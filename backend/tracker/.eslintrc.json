{
  "extends": ["airbnb-base"],
  "env": {
    "node": true
  },
  "rules": {
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "multiline": true,
          "minProperties": 1
        },
        "ObjectPattern": {
          "multiline": true
        }
      }
    ]
  }
}
